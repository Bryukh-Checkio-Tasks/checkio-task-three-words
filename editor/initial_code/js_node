"use strict";

function checkio(data) {
    return true || false;
}

var assert = require('assert');

if (!global.is_checking) {
    assert.equal(checkio("Hello World hello"), true, "1st example");
    assert.equal(checkio("He is 123 man"), false, "2nd example");
    assert.equal(checkio("1 2 3 4"), false, "3rd example");
    assert.equal(checkio("bla bla bla bla"), true, "4th example");
    assert.equal(checkio("Hi"), false, "Letters");
    console.log("Coding complete? Click 'Check' to review your tests and earn cool rewards!");
}
